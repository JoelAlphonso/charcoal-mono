{
    "labels": {
        "name": "Email log",
        "singular_name": "Email log",
        "edit_item": "Edit log",
        "create_item": "Create log",
        "new_item": "New log",
        "all_items": "All logs",
        "menu_items": "Email logs"
    },
    "admin": {
        "export": {
            "default": {
                "properties": [
                    "send_ts",
                    "from",
                    "to",
                    "campaign",
                    "subject",
                    "send_error",
                    "message_id",
                    "ip"
                ],
                "orders": [{
                    "property": "send_ts",
                    "mode": "desc"
                }],
                "exporter_options": {
                    "convert_br_to_newlines": true,
                    "strip_tags": true,
                    "filename": "email-logs.csv"
                }
            }
        },
        "default_export": "default",

        "lists": {
            "default": {
                "title": {
                    "fr": "Logs d'envoi de courriel",
                    "en": "Email logs"
                },
                "properties": [
                    "send_ts",
                    "from",
                    "to",
                    "subject"
                ],
                "orders": [{
                    "property": "send_ts",
                    "mode": "desc"
                }],
                "list_actions": {
                    "export": {
                        "url": "object/export?obj_type={{obj_type}}",
                        "label": {
                            "fr": "Exporter",
                            "en": "Export"
                        }
                    }
                }
            }
        },
        "default_list": "default",

        "forms": {
            "default": {
                "groups": {
                    "charcoal/email/email-log/infos": {
                        "title": {
                            "fr": "Log d'envoi de courriel",
                            "en": "Email log"
                        },
                        "layout": {
                            "structure": [{
                                "columns": [1, 1]
                            },
                                {
                                    "columns": [1, 1]
                                },
                                {
                                    "columns": [1]
                                },
                                {
                                    "columns": [1, 1]
                                }
                            ]
                        },
                        "properties": [
                            "send_ts",
                            "campaign",
                            "from",
                            "to",
                            "subject",
                            "ip",
                            "session_id"
                        ]
                    },
                    "status": {
                        "title": "Status",
                        "properties": [
                            "message_id",
                            "send_status",
                            "send_error"
                        ]
                    }
                }
            }
        },
        "default_form": "default",

        "dashboards": {
            "admin.edit": {
                "layout": {
                    "structure": [{
                        "columns": [1]
                    }]
                },
                "widgets": {
                    "form": {
                        "type": "charcoal/admin/widget/object-form",
                        "form_ident": "default",
                        "obj_type": "charcoal/email/email-log"
                    }
                }
            },
            "admin.table": {
                "layout": {
                    "structure": [{
                        "columns": [1]
                    }]
                },
                "widgets": {
                    "form": {
                        "type": "charcoal/admin/widget/table",
                        "collection_ident": "default",
                        "obj_type": "charcoal/email/email-log"
                    }
                }
            }
        },
        "default_create_dashboard": "admin.edit",
        "default_edit_dashboard": "admin.edit",
        "default_collection_dashboard": "admin.table"
    }
}
